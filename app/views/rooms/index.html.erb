<p style="color: green"><%= notice %></p>

<% content_for :title, "Rooms" %>

<h1>Rooms</h1>

<div id="rooms">
  <% @rooms.each do |room| %>
    <div class="table_entry">
      <%= render room %>
      <p>
        <%= link_to "Show this room", room %>
      </p>
    </div>
  <% end %>
</div>

<%= link_to "New room", new_room_path %>
