<div id="<%= dom_id student %>">
  <p>
    <strong>First name:</strong>
    <%= student.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= student.last_name %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= student.age %>
  </p>

  <p>
    <strong>Grade level:</strong>
    <%= student.grade_level %>
  </p>

  <p>
    <strong>Grade average:</strong>
    <%= student.grade_average %>
  </p>

  <p>
    <strong>Emergency contact:</strong>
    <%= student.emergency_contact %>
  </p>

  <p>
    <strong>Credits obtained:</strong>
    <%= student.credits_obtained %>
  </p>

  <p>
    <strong><%= link_to "Courses", courses_path(student_id: student.id) %></strong>
  </p>

</div>
